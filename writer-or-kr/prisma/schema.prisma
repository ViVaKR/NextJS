generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Demo {
  id    Int    @id @default(autoincrement())
  title String

  @@map("demos")
}

model Memo {
  id   Int    @id @default(autoincrement())
  memo String

  @@map("memos")
}
